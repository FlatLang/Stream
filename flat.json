{
  "name": "Stream",
  "version": "0.1.0",
  "description": "Flat Stream Library",
  "author": "Braden Steffaniak",
  "license": "ISC",
  "defaultTarget": "es6",
  "scripts": {
    "test": "node dist/es6/test.js"
  },
  "sources": {
    "main": {
      "source": "src",
      "defaultExports": [
        {
          "location": "flat/stream/StreamListExtensions",
          "static": true
        }
      ],
      "dependencies": {
        "https://github.com/FlatLang/System": "master",
        "https://github.com/FlatLang/IO": "master",
        "https://github.com/FlatLang/Datastruct": "master",
        "https://github.com/FlatLang/Logger": "master"
      }
    },
    "test": {
      "mainClass": "flat/stream/Stream_TestSuite",
      "source": "src/**/*_Test*.flat",
      "dependencies": {
        "https://github.com/FlatLang/Test": "master",
        "https://github.com/FlatLang/Thread": "master",
        "https://github.com/FlatLang/Time": "master"
      }
    }
  }
}
